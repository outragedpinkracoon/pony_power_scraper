{
	"extends": "google",
	"parserOptions": {
		"ecmaVersion": 2017,
		"sourceType": "module"
	},
	"rules": {
		"semi": [2, "never"],
		"no-multiple-empty-lines": [
		"error",
			{
				"max": 1,
				"maxEOF": 1
			}
		],
		"comma-dangle": "off",
		"object-curly-spacing": "off",
		"require-jsdoc":"off"
	}
}
